<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mkim11235.gainztracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AddExerciseActivity"
            android:parentActivityName=".MainActivity">
        </activity>

        <activity
            android:name=".ExerciseHistoryActivity"
            android:parentActivityName=".MainActivity"
            android:launchMode="singleTop">
        </activity>

        <activity
            android:name=".ExerciseHistoryEntryActivity.AddExerciseHistoryEntryActivity"
            android:parentActivityName=".ExerciseHistoryActivity">
        </activity>

        <activity
            android:name=".ExerciseHistoryEntryActivity.EditExerciseHistoryEntryActivity"
            android:parentActivityName=".ExerciseHistoryActivity">
        </activity>

        <provider
            android:authorities="com.example.mkim11235.gainztracker"
            android:name=".data.ExerciseProvider" />
    </application>

</manifest>